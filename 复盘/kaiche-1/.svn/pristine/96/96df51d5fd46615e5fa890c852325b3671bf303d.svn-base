*,*::before,*::after{
  box-sizing: border-box;
}
.content-main{
  width     : 100%;
  background: #efefef;
  height    : 1000px;
  >.main-top{
    // height       : 250px;
    width  : 100%;
    padding: 15px;
    // border-bottom: 1px solid #7c7c7c;
    >.main-search{
      background: #fff;
      border    : 1px solid #000;
      overflow  : hidden;
    }
  }
  >.main-bottom{
    width: 100%;
    // height : 530px;
    padding: 15px;
    /deep/ .ant-table-tbody>tr>td{
      background: #fff;
    }
    >.list{
      border: 1px solid #000;
      >.list-head{
        border-bottom: 1px solid #000;
        padding      : 10px;
      }
    }
  }
}
